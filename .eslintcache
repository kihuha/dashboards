e?: boolean\n  color?: string\n  background?: boolean\n  backgroundOpacity?: number\n}\n\nconst Pie = ({\n  data,\n  thickness,\n  pie,\n  color,\n  background,\n  backgroundOpacity,\n}: PieProps) => {\n  const ref = useRef(null)\n  const backRef = useRef(null)\n  const [itemRefs, setItemRefs] = useState<any>([])\n\n  const generateArc = useCallback(\n    (\n      percent: number,\n      startAngle: number = 0,\n      endAngle: number = Math.PI * 2 * percent\n    ): string => {\n      const arc: any = d3\n        .arc()\n        .innerRadius(100 - (pie ? 100 : thickness || 0))\n        .outerRadius(100)\n        .startAngle(startAngle)\n        .endAngle(endAngle)\n\n      return arc()\n    },\n    [pie, thickness]\n  )\n\n  const generateMultipleChart = useCallback(() => {\n    const pie = d3.pie().value((d: any) => d.value / 100)\n\n    pie(data).map((segment: any, index) => {\n      const arc = generateArc(\n        segment.value,\n        segment.startAngle,\n        segment.endAngle\n      )\n      if (itemRefs[index])\n        d3.select(itemRefs[index].current)\n          .attr(\"d\", arc)\n          .style(\"transform\", \"translate(50%, 50%)\")\n          .style(\"fill\", segment.data.color)\n    })\n  }, [data, generateArc, itemRefs])\n\n  const generateChart = useCallback(\n    (parent: any) => {\n      const arc: any = generateArc(data / 100)\n\n      d3.select(parent)\n        .attr(\"d\", arc)\n        .style(\"transform\", \"translate(50%, 50%)\")\n        .attr(\"fill\", color || \"\")\n\n      if (backRef.current) {\n        const backArc: any = generateArc(1)\n\n        d3.select(backRef.current)\n          .attr(\"d\", backArc)\n          .style(\"transform\", \"translate(50%, 50%)\")\n          .attr(\n            \"fill\",\n            `${\n              background ? getRGBA(color, backgroundOpacity) : getRGBA(color, 0)\n            }`\n          )\n      }\n    },\n    [data, color, background, backgroundOpacity, generateArc]\n  )\n\n  useEffect(() => {\n    if (typeof data === \"object\") {\n      const refs = data.map((item: any) => createRef())\n      setItemRefs(refs)\n    }\n  }, [data])\n\n  useEffect(() => {\n    if (ref.current && typeof data !== \"object\") {\n      generateChart(ref.current)\n    } else {\n      generateMultipleChart()\n    }\n  }, [generateChart, generateMultipleChart, data])\n\n  return (\n    <svg viewBox={`0 0 200 200`} preserveAspectRatio=\"xMidYMid meet\">\n      <g>\n        {typeof data === \"object\" ? (\n          itemRefs.map((item: any) => <path ref={item} d=\"\" />)\n        ) : (\n          <path ref={ref} d=\"\" />\n        )}\n      </g>\n      <g>\n        <path ref={backRef} d=\"\" />\n      </g>\n    </svg>\n  )\n}\n\nexport default Pie\n",["91","92"],"/home/jenga/Documents/personal/dashboards/src/stories/Pie.stories.tsx",[],"/home/jenga/Documents/personal/dashboards/src/chartUtils/color.ts",[],"/home/jenga/Documents/personal/dashboards/src/design/color.ts",[],"/home/jenga/Documents/personal/dashboards/src/components/button.tsx",[],"/home/jenga/Documents/personal/dashboards/src/pages/Landing.tsx",[],"/home/jenga/Documents/personal/dashboards/src/design/font.ts",[],"/home/jenga/Documents/personal/dashboards/src/components/avatar.tsx",[],"/home/jenga/Documents/personal/dashboards/src/stories/Avatar.stories.tsx",[],"/home/jenga/Documents/personal/dashboards/src/components/images.ts",[],"/home/jenga/Documents/personal/dashboards/src/components/progress.tsx",[],"/home/jenga/Documents/personal/dashboards/src/stories/Progress.stories.tsx",[],"/home/jenga/Documents/personal/dashboards/src/stories/Sidebar.stories.tsx",[],"/home/jenga/Documents/personal/dashboards/src/compositions/sidebar.tsx",[],"/home/jenga/Documents/personal/dashboards/src/components/iconButton.tsx",[],"/home/jenga/Documents/personal/dashboards/src/utils.ts",[],{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"93","replacedBy":"97"},{"ruleId":"95","replacedBy":"98"},{"rule